{
  "name": "@percy/agent",
  "description": "A low-level Node process for interacting with Percy.",
  "version": "0.0.1",
  "author": "Perceptual Inc",
  "bin": {
    "percy-agent": "./bin/run"
  },
  "bugs": "https://github.com/percy/percy-agent/issues",
  "dependencies": {
    "@oclif/command": "^1.4.6",
    "@oclif/config": "^1.3.62",
    "@oclif/plugin-help": "^1.2.2",
    "@oclif/plugin-not-found": "^1.0.5",
    "@types/express": "^4.11.1",
    "body-parser": "^1.18.2",
    "cors": "^2.8.4",
    "express": "^4.16.3",
    "percy-client": "^2.8.1"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.4.4",
    "@oclif/test": "^1.0.1",
    "@oclif/tslint": "^1.0.2",
    "@types/chai": "^4.1.2",
    "@types/chai-http": "^3.0.4",
    "@types/cors": "^2.8.3",
    "@types/mocha": "^5.0.0",
    "@types/nock": "^9.1.3",
    "@types/node": "^9.6.1",
    "axios": "^0.18.0",
    "chai": "^4.1.2",
    "chai-http": "^4.0.0",
    "globby": "^8.0.1",
    "husky": "^1.0.0-rc.1",
    "mocha": "^5.0.5",
    "nock": "^9.2.5",
    "prettier": "1.12.1",
    "pryjs": "^1.0.3",
    "stdout-stderr": "^0.1.9",
    "testem": "^2.2.1",
    "ts-node": "5",
    "tsify": "^4.0.0",
    "tslib": "^1.9.0",
    "tslint": "^5.9.1",
    "typescript": "^2.8.1"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    ".oclif.manifest.json",
    "package.json",
    "/bin",
    "/dist"
  ],
  "homepage": "https://github.com/percy/percy-agent",
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint"
    }
  },
  "keywords": [
    "oclif",
    "percy",
    "agent"
  ],
  "license": "TBD",
  "main": "dist/index.js",
  "oclif": {
    "commands": "./dist/commands",
    "bin": "percy-agent",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-not-found"
    ]
  },
  "repository": "percy/percy-agent",
  "scripts": {
    "build": "rm -rf dist && tsc && yarn run build-client",
    "build-client": "rm -rf dist/public && tsc -p src/percy-agent-client",
    "build-test-client": "browserify test/percy-agent-client/*.ts -p [ tsify --noImplicitAny ] > test/dist/bundle.js",
    "build-test-client2": "tsc -p test/percy-agent-client",
    "clean": "rm -f .oclif.manifest.json",
    "lint": "tsc -p test --noEmit && tslint -p test -t stylish --fix",
    "postpublish": "yarn run clean",
    "posttest": "yarn run lint",
    "prepublishOnly": "yarn run build && oclif-dev manifest && oclif-dev readme",
    "preversion": "yarn run clean",
    "test": "yarn run build-client && PERCY_PROJECT=test/test mocha --forbid-only \"test/**/*.test.ts\"",
    "test-client": "yarn run build-client && PERCY_PROJECT=test/test testem",
    "version": "oclif-dev readme && git add README.md"
  },
  "types": "dist/index.d.ts"
}